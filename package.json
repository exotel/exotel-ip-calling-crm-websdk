{
  "name": "@exotel-npm-dev/exotel-ip-calling-crm-websdk",
  "version": "1.3.0",
  "description": "Exotel's crm websdk for IP calling integration",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "browser": "./output/index.js",
  "exports": {
    ".": {
      "types": "./output/index.d.ts",
      "import": "./output/index.js",
      "default": "./output/index.js"
    },
    "./bundle": "./target/crmBundle.js",
    "./src/*": "./src/*",
    "./output/*": "./output/*",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "output/",
    "src/",
    "target/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "bundle": "webpack --config webpack.config.js",
    "build:all": "npm run build && npm run bundle",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExoAbhishek/exotel-ip-calling-crm-websdk.git"
  },
  "author": "Exotel",
  "license": "Apache-2.0",
  "dependencies": {
    "@exotel-npm-dev/webrtc-client-sdk": "^3.0.4",
    "crypto-js": "^4.1.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@types/crypto-js": "^4.2.2",
    "babel-loader": "^9.1.3",
    "circular-dependency-plugin": "^5.2.2",
    "file-loader": "^6.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.0.3",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  }
}
